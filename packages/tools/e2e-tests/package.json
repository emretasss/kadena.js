{
  "name": "@kadena-dev/e2e-tests",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:md": "remark README.md -o --use @kadena-dev/markdown",
    "format:src": "prettier . --cache --write",
    "install:chromium": "npx playwright install chromium",
    "generate:faucet:contract": "pactjs contract-generate --contract user.coin-faucet --api https://api.testnet.chainweb.com/chainweb/0.0/testnet04/chain/1/pact --chain 1 --network testnet",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .js,.ts",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@kadena/client": "workspace:*",
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/heft-rig": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/markdown": "workspace:*",
    "@playwright/test": "^1.39.0",
    "@rushstack/eslint-config": "~3.3.0",
    "@types/node": "^18.17.14",
    "eslint": "^8.45.0",
    "eslint-plugin-playwright": "0.16.0",
    "prettier": "~3.0.3",
    "@kadena/cryptography-utils": "^0.3.8",
    "@kadena/chainweb-node-client": "workspace:*",
    "@kadena/pactjs-cli": "workspace:*",
    "@kadena/types": "workspace:*"
  }
}
